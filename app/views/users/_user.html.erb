<div class="container">
  <table class="table table-striped table-bordered">
    <thead class="thead-dark">
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th># of Books Read</th>
        
      </tr>
    </thead>

    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.email %></td>
          <td> Holder </td>
          </tr>
      <% end %>
    </tbody>
  </table>

  <p>
    <%= link_to 'Add new book', new_book_path %>
  </p>
</div>
</div>